(set-logic HORN)
(declare-rel itp (Int Int Int))
(declare-rel simple!!query ())
(declare-var A Int)
(declare-var B Int)
(declare-var C Int)
(declare-var D Int)
(declare-var E Int)
(declare-var F Int)
(rule (=> (and (= A 0) (= B 0) (= C 0)) (itp A B C)))
(rule (=> (and (itp A B C) (= D (+ A 1)) (= E (+ B 1)) (= F (+ C 1))) (itp D E F)))
(rule (=> (and (itp A B C) (>= A 10000) (not (= B C))) simple!!query))
(query simple!!query)
